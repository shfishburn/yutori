{
    "version": 2,
    "builds": [
      {
        "src": "workouts/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "editor/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/workouts/(.*)",
        "dest": "/workouts/$1"
      },
      {
        "src": "/editor/(.*)",
        "dest": "/editor/$1"
      },
      {
        "src": "/",
        "dest": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "hasHeaders": [
          {
            "key": "Content-Security-Policy",
            "value": "default-src 'self'; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com https://cdnjs.cloudflare.com; script-src 'self' 'unsafe-inline' https://cdn.jsdelivr.net; font-src 'self' https://fonts.gstatic.com; img-src 'self' data:; connect-src 'self'"
          }
        ]
      }
    ]
  }