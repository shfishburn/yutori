{
    "version": 2,
    "builds": [
      {
        "src": "workouts/**/*.html",
        "use": "@vercel/static"
      },
      {
        "src": "workouts/**/*.js",
        "use": "@vercel/static"
      },
      {
        "src": "workouts/**/*.css",
        "use": "@vercel/static"
      },
      {
        "src": "workouts/**/*.json",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/workouts/$1"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "default-src 'self'; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com https://cdnjs.cloudflare.com; script-src 'self' 'unsafe-inline' https://cdn.jsdelivr.net; font-src 'self' https://fonts.gstatic.com; img-src 'self';"
          }
        ]
      }
    ]
  }
  