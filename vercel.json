{
    "version": 2,
    "builds": [
      {
        "src": "workouts/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "editor/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/workouts/(.*)",
        "dest": "/workouts/$1"
      },
      {
        "src": "/editor/(.*)",
        "dest": "/editor/$1"
      },
      {
        "src": "/",
        "dest": "/index.html"
      }
    ]
  }