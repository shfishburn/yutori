{
    "version": 2,
    "builds": [
      {
        "src": "assets/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "calcv1/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "chat/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "editor/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "legal/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "workouts/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "*.{html,css,js}",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/calcv1/(.*)",
        "dest": "/calcv1/$1"
      },
      {
        "src": "/chat/(.*)",
        "dest": "/chat/$1"
      },
      {
        "src": "/editor/(.*)",
        "dest": "/editor/$1"
      },
      {
        "src": "/legal/(.*)",
        "dest": "/legal/$1"
      },
      {
        "src": "/workouts/(.*)",
        "dest": "/workouts/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }