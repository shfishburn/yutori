{
  "version": 2,
  "routes": [
    {
      "src": "/bcc",
      "dest": "/bcc/index.html"
    },
    {
      "src": "/bcc/",
      "dest": "/bcc/index.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/src/js/(.*)",
      "dest": "/src/js/$1"
    },
    {
      "src": "/src/styles/(.*)",
      "dest": "/src/styles/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
